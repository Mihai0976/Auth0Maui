<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="Auth0Maui.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:Auth0Maui.Views"
	xmlns:local="clr-namespace:Auth0Maui">

	<TabBar>
		<ShellContent
			Title="Main"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:MainPage}" />
		<ShellContent
			Title="ListDetail"
			Icon="iconlistdetail.png"
			ContentTemplate="{DataTemplate views:ListDetailPage}" />
		<ShellContent
			Title="Map"
			Icon="iconmap.png"
			ContentTemplate="{DataTemplate views:MapPage}" />
	</TabBar>
</Shell>
